{
  "name": "calculate",
  "displayName": "Calculate",
  "description": "Select some math, evaluate it",
  "version": "1.1.1",
  "publisher": "acarreiro",
  "engines": {
    "vscode": "^0.10.6"
  },
  "author": {
    "name": "Andrew Carreiro",
    "email": "andrew@headfirst.ca",
    "url": "http://headfirst.ca/"
  },
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewcarreiro/vscode-calculate"
  },
  "icon": "store-icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.calculate",
        "title": "Calculate"
      },
      {
        "command": "extension.calculateReplace",
        "title": "Calculate and Replace"
      },
      {
        "command": "extension.count",
        "title": "Count"
      },
      {
        "command": "extension.countAlpha",
        "title": "Count Alphabetical"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "20.0.0",
    "jest": "20.0.0",
    "typescript": "2.0.6",
    "vscode": "0.11.14"
  },
  "dependencies": {
    "mathjs": "3.3.0"
  },
  "jest": {
    "testRegex": "/out/test/.*\\.(ts|tsx|js)$"
  }
}
